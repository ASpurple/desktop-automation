{
	"include": ["index.d.ts", "*.ts"],
	"exclude": ["node_modules", "dist"],
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "node",
		"rootDir": ".",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true
	}
}
